          <p>Se vende</p>
          <% } else { %>
          <p>Se busca</p>
          <% } %>
          <p><%= anuncio.price %>â‚¬</p>
          <p><%= anuncio.tags %></p>
          <p><%= anuncio.photo %></p>
        </li>
    <% }) %>
  </ul>
  </body>
</html>
